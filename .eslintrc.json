{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style":"off",
        "no-console":"off",
        "indent":"off",
        "object-curly-newline":"off",
        "no-trailing-spaces":"off",
        "max-len":"off",
        "eqeqeq":"off",
        "object-shorthand":"off",
        "no-underscore-dangle":"off",
        "arrow-parens":"off",
        "newline-per-chained-call":"off"
    }
}
